  "engines": {
    "node": "^10 || ^12 || >=14"
  },
  "keywords": [
    "autoprefixer",
    "css",
    "prefix",
    "postcss",
    "postcss-plugin"
  ],
  "main": "lib/autoprefixer.js",
  "bin": "bin/autoprefixer",
  "funding": {
