  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "postcss/autoprefixer",
  "peerDependencies": {
    "postcss": "^8.1.0"
  },
  "dependencies": {
    "browserslist": "^4.14.7",
    "caniuse-lite": "^1.0.30001161",
    "colorette": "^1.2.1",
    "normalize-range": "^0.1.2",
    "num2fraction": "^1.2.2",
    "postcss-value-parser": "^4.1.0"
  },
  "browser": {
